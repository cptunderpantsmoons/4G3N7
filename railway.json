{
  "$schema": "https://railway.app/railway.schema.json",
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "on_failure",
    "startCommand": "node packages/4g3n7-goose-bridge/dist/main.js",
    "healthCheck": {
      "interval": 30,
      "timeout": 10,
      "path": "/health"
    }
  },
  "environmentVariables": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info",
    "API_PORT": "3000"
  }
}
